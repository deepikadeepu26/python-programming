class PeekingIterator:
    def __init__(self, iterator):
        self.arr = iterator.v
        self.pointer = 0

    def peek(self):
        return self.arr[self.pointer]
        
    def next(self):
        next_element = self.arr[self.pointer]
        self.pointer += 1
        return next_element

    def hasNext(self):
        return self.pointer < len(self.arr)
